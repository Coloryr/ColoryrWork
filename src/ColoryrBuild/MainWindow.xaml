                    <TabItem Header="Socket" Style="{DynamicResource TabItem}">
                        <Grid>
                            <Rectangle
                                Fill="#FFF4F4F5"
                                Stroke="#FF5ABED6"
                                StrokeThickness="2" />
                            <ListView
                                Name="ListSocket"
                                Height="361"
                                Margin="2,2,2,0"
                                VerticalAlignment="Top">
                                <ListView.View>
                                    <GridView>
                                        <GridViewColumn
                                            Width="280"
                                            DisplayMemberBinding="{Binding UUID}"
                                            Header="UUID" />
                                        <GridViewColumn
                                            Width="240"
                                            DisplayMemberBinding="{Binding Text}"
                                            Header="说明" />
                                        <GridViewColumn
                                            Width="40"
                                            DisplayMemberBinding="{Binding Version}"
                                            Header="版本" />
                                        <GridViewColumn
                                            Width="180"
                                            DisplayMemberBinding="{Binding CreateTime}"
                                            Header="创建时间" />
                                        <GridViewColumn
                                            Width="180"
                                            DisplayMemberBinding="{Binding UpdataTime}"
                                            Header="修改时间" />
                                    </GridView>
                                </ListView.View>
                                <ListView.ContextMenu>
                                    <ContextMenu>
                                        <MenuItem Click="Add_Socket_Click" Header="添加" />
                                        <MenuItem Click="Change_Socket_Click" Header="修改" />
                                        <MenuItem Click="Delete_Socket_Click" Header="删除" />
                                    </ContextMenu>
                                </ListView.ContextMenu>
                            </ListView>
                            <Label
                                Margin="2,363,0,0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Top"
                                Content="搜索" />
                            <TextBox
                                Name="InputSocket"
                                Width="594"
                                Margin="44,367,0,0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Top"
                                TextChanged="Input_Socket_TextChanged"
                                TextWrapping="Wrap" />
                            <Button
                                Width="55"
                                Margin="643,368,0,0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Top"
                                Click="Clear_Socket_Click"
                                Content="清空" />
                            <Button
                                Width="55"
                                Margin="703,368,0,0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Top"
                                Click="Add_Socket_Click"
                                Content="添加" />
                            <Button
                                Width="54"
                                Margin="763,368,0,0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Top"
                                Click="Change_Socket_Click"
                                Content="修改" />
                            <Button
                                Width="55"
                                Margin="822,367,0,0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Top"
                                Click="Re_Socket_Click"
                                Content="刷新" />
                        </Grid>
