<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net5.0</TargetFramework>
    <StartupObject></StartupObject>
    <Platforms>x64</Platforms>
    <ApplicationIcon>ColoryrWork.ico</ApplicationIcon>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|x64'">
    <OutputPath>..\..\build_out\ServerRun</OutputPath>
    <PlatformTarget>x64</PlatformTarget>
    <Optimize>false</Optimize>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|x64'">
    <Optimize>false</Optimize>
    <PlatformTarget>x64</PlatformTarget>
    <OutputPath>..\build_out\</OutputPath>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\ColoryrServer\ColoryrServer.csproj" />
  </ItemGroup>

  <Target Name="PostBuild" AfterTargets="PostBuildEvent">
    <Exec Command="del &quot;$(TargetDir)ColoryrServer.RunTest.deps.json&quot;&#xD;&#xA;del &quot;$(TargetDir)ColoryrServer.deps.json&quot;&#xD;&#xA;copy &quot;$(TargetDir)runtimes\win\lib\netcoreapp3.0\System.Drawing.Common.dll&quot; &quot;$(TargetDir)System.Drawing.Common.dll&quot;&#xD;&#xA;" />
  </Target>

</Project>
