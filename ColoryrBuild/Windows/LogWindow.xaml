<Window x:Class="ColoryrBuild.Windows.LogWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ColoryrBuild.Windows"
        mc:Ignorable="d"
        Title="日志窗口" MinHeight="208" MinWidth="400" Height="208" Width="400" Closed="Window_Closed">
    <Grid>
        <Rectangle Fill="#FFF4F4F5" Stroke="#FF5ABED6" StrokeThickness="2"/>
        <TextBox TextWrapping="Wrap" VerticalScrollBarVisibility="Visible" IsReadOnly="True" Margin="3,3,3,3" Name="Text"/>
        
    </Grid>
</Window>
